<TubingGui size="54" id="players-overview">
    <title class="gui-title">Online players</title>

    <GuiItem id="player-info-0" class="player-info" slot="0" onLeftClick="players/view/detail?targetPlayerName=player1" onRightClick="$NOOP" onLeftShiftClick="$NOOP" material="PLAYER_HEAD">
        <name class="item-name" color="&amp;5">player1</name>
        <Lore>

            <LoreLine permission="staff.player-view.detail.uuid">
                <t id="uuid-label" class="detail-label" color="&amp;b">UUID: </t>
                <t id="uuid-value" class="detail-value" color="&amp;6">a8525382-49ed-4c9c-9c50-c00a0e570159</t>
            </LoreLine>



            <LoreLine>
                <t color="&amp;7" class="line-separator">---------------</t>
            </LoreLine>
            <LoreLine if="false">
                <t id="online-label" color="&amp;2">ONLINE</t>
            </LoreLine>
            <LoreLine if="true">
                <t id="offline-label" color="&amp;C">OFFLINE</t>
            </LoreLine>
            <LoreLine></LoreLine>
            <LoreLine>&amp;7Left click to &amp;6view players details</LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="player-info-1" class="player-info" slot="1" onLeftClick="players/view/detail?targetPlayerName=player2" onRightClick="$NOOP" onLeftShiftClick="$NOOP" material="PLAYER_HEAD">
