    <GuiItem id="ban-info" class="ban-info" slot="13" onLeftClick="$NOOP" onRightClick="$NOOP" onMiddleClick="$NOOP" material="PLAYER_HEAD">
        <name class="item-name" color="&amp;C">Ban</name>
        <Lore>
            <LoreLine>
                <t color="&amp;b" id="id-label" class="detail-label">Id: </t>
                <t color="&amp;6" id="id-value" class="detail-value">12</t>
            </LoreLine>

            <LoreLine if="true">
                <t color="&amp;b" id="server-label" class="detail-label">Server: </t>
